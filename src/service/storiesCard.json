            { "start": 0.1, "end": 3.5 , "text": "As Cores da Amizade" },
            { "start": 4, "end": 10, "text": "Havia uma menina chamada Clara," },
            { "start": 10, "end": 14.5, "text": "Um dia, o professor trouxe" },
