            { "start": 18, "end": 21, "text": "e pediu que cada aluno" },
            { "start": 21, "end": 25, "text": "pintasse um sentimento." },
            { "start": 24, "end": 32, "text": "Clara escolheu o amarelo" },
            { "start": 32, "end": 34, "text": "para a alegria" },
            { "start": 34, "end": 38, "text": "o azul para a paz" },
            { "start": 38, "end": 40, "text": "e o vermelho para o amor." },
            { "start": 40, "end": 50, "text": "Seu amigo Rafael, porém, escolheu o preto" },
            { "start": 50, "end": 55, "text": "Ele disse que estava triste." },
            { "start": 55, "end": 63, "text": "Clara olhou para ele e teve uma ideia." },
            { "start": 64, "end": 67, "text": "Misturou o amarelo e o azul," },
            { "start": 68, "end": 72, "text": "formando um verde de esperança" },
            { "start": 73, "end": 80, "text": "Clara desenhou um arco-íris e escreveu:" },
        { "start": 81, "end": 85, "text": "\"Amigos fazem o mundo colorido!\"" },
            { "start": 86, "end": 95, "text": "Rafael sorriu e também começou a pintar." },
            { "start": 95, "end": 100, "text": "O professor observou e disse:" },
            { "start": 100, "end": 105, "text": "\"O melhor jeito de dizer que ama" },
            { "start": 105, "end": 109, "text": "é com um gesto colorido.\"" }
        ]
